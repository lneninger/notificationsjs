{
  "compileOnSave": true,
  "compilerOptions": {
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "es5",
    "rootDir": "./src",
    "outDir": "dist"
  },
  "exclude": [
    "dist",
    "node_modules",
    "firebase-functions"
  ]

}
